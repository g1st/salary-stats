{
  "name": "salary-stats",
  "description": "Analyse and compare salaries by cohort.",
  "keywords": [
    "salary",
    "statistics",
    "comparison",
    "angular",
    "highcharts",
    "bulma"
  ],
  "version": "0.0.1",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "build": "ng build --prod",
    "codacy": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage -p .",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "pree2e": "webdriver-manager update --gecko false",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "reinstall": "rm -rf ./node_modules && npm install",
    "ship": "npm run lint && npm run test && npm run e2e",
    "start": "ng serve",
    "test": "ng test --no-watch",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "test:watch": "ng test"
  },
  "private": true,
  "author": "Jonathan Sharpe",
  "contributors": [
    "Beat Richartz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/textbook/salary-stats"
  },
  "homepage": "https://github.com/textbook/salary-stats",
  "bugs": "https://github.com/textbook/salary-stats/issues",
  "dependencies": {
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/forms": "9.1.11",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "angular-in-memory-web-api": "0.11.0",
    "highcharts": "^8.1.2",
    "highcharts-angular": "^2.4.0",
    "jerzy": "^0.2.1",
    "rxjs": "^6.5.5",
    "simple-statistics": "^7.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "9.1.11",
    "@angular/language-service": "9.1.11",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^5.1.2",
    "coveralls": "^3.1.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.0.3",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "node-sass": "^4.14.1",
    "protractor": "^7.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^6.1.2",
    "typescript": "3.8.3"
  }
