    "@angular/language-service": "7.2.16",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.108",
    "codacy-coverage": "^2.1.1",
    "codelyzer": "^4.3.0",
