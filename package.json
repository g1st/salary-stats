{
  "name": "salary-stats",
  "description": "Analyse and compare salaries by cohort.",
  "keywords": [
    "salary",
    "statistics",
    "comparison",
    "angular",
    "highcharts",
    "bulma"
  ],
  "version": "0.0.1",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "codacy": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage -p .",
    "test": "ng test --no-watch",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "test:watch": "ng test",
    "pree2e": "webdriver-manager update --gecko false",
    "e2e": "ng e2e",
    "reinstall": "rm -rf ./node_modules && npm install",
    "build": "ng build --prod"
  },
  "private": true,
  "author": "Jonathan Sharpe",
  "contributors": [
    "Beat Richartz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/textbook/salary-stats"
  },
  "homepage": "https://github.com/textbook/salary-stats",
  "bugs": "https://github.com/textbook/salary-stats/issues",
  "dependencies": {
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "angular-in-memory-web-api": "^0.6.0",
    "angular2-highcharts": "^0.5.5",
    "core-js": "^2.5.5",
    "intl": "^1.2.5",
    "jerzy": "^0.2.1",
    "rxjs": "^6.1.0",
    "simple-statistics": "^5.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.108",
    "codacy-coverage": "^2.1.1",
    "codelyzer": "^4.3.0",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  }
}
